<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
// Use the same list display from Views/Search/Index.cshtml
    @foreach (var job in ViewBag.jobs)
    {
        <table>
            @foreach (var jobInfo in job)
            {
                <tr>
                    <td>@jobInfo.Key</td>
                    <td>@jobInfo.Value</td>
                </tr>

            }
        </table>
        <br />
    }
}